{
  "buildCommand": "cd web-app && npm run build",
  "devCommand": "cd web-app && npm run dev",
  "outputDirectory": "web-app/out",
  "framework": "nextjs",
  "installCommand": "cd web-app && npm install",
  "trailingSlash": true,
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "Referrer-Policy",
          "value": "origin-when-cross-origin"
        }
      ]
    }
  ]
}